export VAGRANT_HTTP_PROXY VAGRANT_HTTPS_PROXY VAGRANT_APT_HTTP_PROXY VAGRANT_APT_HTTPS_PROXY VAGRANT_NO_PROXY

export VAGRANT_DEFAULT_PROVIDER=virtualbox
