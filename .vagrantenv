NO_PROXY=$no_proxy
VAGRANT_NO_PROXY=$no_proxy

export http_proxy https_proxy HTTP_PROXY HTTPS_PROXY no_proxy NO_PROXY

VAGRANT_HTTP_PROXY=$http_proxy
